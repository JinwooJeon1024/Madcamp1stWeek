<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="AppTheme" parent="Theme.AppCompat.Light.DarkActionBar">
        <!-- Customize your theme here. -->
        <item name="android:fontFamily">@font/soyoorginal</item>
        <item name="colorPrimary">#FF695E</item>
        <item name="colorPrimaryDark">@color/black</item>
        <item name="colorAccent">@color/colorAccent</item>
    </style>
    <style name="MyButtonStyle" parent="Widget.AppCompat.Button">
        <item name="android:textColor">#FFFFFF</item>
        <item name="android:background">@drawable/rounded_corners</item>
    </style>
    <style name="RoundedCorners">
        <item name="cornerSize">90dp</item>
    </style>
    <style name="DialogAnimation">
        <item name="android:windowEnterAnimation">@anim/dialog_scale</item>
        <item name="android:windowExitAnimation">@anim/dialog_scale</item>
    </style>

</resources>
